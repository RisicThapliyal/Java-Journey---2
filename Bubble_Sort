import java.util.Scanner;

public class Sorting {
public static void main(String[] args) {
int[] arr = {4 , 2 , 7 , 3 , 9};

        for (int i = 0; i < arr.length; i++){
            for (int j = 0; j < arr.length-i-1; j++){
                if (arr[j] > arr[j + 1]){
                    int temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                }
            }
            System.out.println(arr[i]);
        }
    }
}
